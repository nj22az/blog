{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n SANITY,WEB -c blue,green \"npm run dev:sanity\" \"wait-on http://localhost:3333 && npm run dev:web\"",
    "dev:sanity": "sanity dev --port 3333",
    "dev:web": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint '{src,__tests__}/**/*.{ts,tsx}' --fix",
    "lint:check": "eslint '{src,__tests__}/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,md,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,css,md,json}'",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "tree": "tree --dirsfirst -I node_modules > FILE_TREE.md",
    "sanity:build": "sanity build",
    "sanity:deploy": "sanity deploy",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@portabletext/react": "^3.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
